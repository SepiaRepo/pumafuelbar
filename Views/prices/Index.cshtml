@model pumafuelbar.Controllers.list

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
       Previous rates:  @Model.lv
                @*@Html.DisplayNameFor(model => model.rate)*@
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
      <tr>
         <td>
  
               
                   @try
                {
                    if (Model.fv > Model.lv)
                    {
                        <p style="color:red">
                            @Model.fv
                            <img src="~/css/media/up.png" />
                        </p>
                    }
                    else
                    {
                        <p style="color:green">
                            @Model.fv
                            <img src="~/css/media/down.png" />
                        </p>
                    }
                }
                catch
                {
                    <p>@Model.nullvalue</p>
                }

            
                            
             
            </td>
         @*<td>
                <a asp-action="Edit" asp-route-id="@item.id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.id">Delete</a>
         </td>*@
        </tr>

    </tbody>
</table>
